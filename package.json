{
  "name": "boiler-plate-express-mongodb",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "start:debug": "nodemon --inspect",
    "start:prod": "node ./app.js",
    "test": "jest --config config/jest.json",
    "test:watch": "jest --watch --config config/jest.json",
    "test:coverage": "jest --config config/jest.json --collectCoverage && codecov",
    "start:doc": "babel-node --extensions \".ts\" swagger/app.docs.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist --extensions \".ts\"",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "prettier": "prettier --config .prettierrc --write src/**/*.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.3",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.9.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.4",
    "@types/mongoose": "^5.7.3",
    "@types/node": "^12.12.7",
    "@types/pino": "^5.15.5",
    "codecov": "^3.6.5",
    "coveralls": "^3.0.11",
    "husky": "^4.2.5",
    "jest": "^25.1.0",
    "jest-express": "^1.10.1",
    "lint-staged": "^10.1.3",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run tslint",
      "npm run prettier"
    ]
  }
}
